generator client {
  provider = "prisma-client-js"
}

generator sqlSchema {
  provider = "bun ./generators/sql-schema-generator.ts"
  output   = "../src/generated"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["video"]
}

model Video {
  id    Int    @id @default(autoincrement())
  titel String @unique

  @@schema("video")
}
